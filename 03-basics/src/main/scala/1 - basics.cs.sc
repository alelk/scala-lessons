1           // Int
1.1         // Double
"Hello"     // String
true        // Boolean

1 + 2 * 3   // 7
1.+(2.*(3)) // 7

1 * 2 + 3   // 5
1.*(2).+(3) // 5

/** | Scala   |  Java     |
  * -----------------------
  * | Int     |  int      |
  * | Double  |  double   |
  * | Long    |  long     |
  * | Boolean |  boolean  |
  */

(1: Int).toDouble // 1.0
1.2.toInt         // 1
"123".toInt       // 123
"true".toBoolean  // true

